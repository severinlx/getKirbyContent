{
   "containerDefinitions": [ 
      {
         "essential": true,
         "image": "478526318168.dkr.ecr.eu-central-1.amazonaws.com/vacation-backend:6a9a41d17e178b1a3590247c212f38180d58d827",
         "logConfiguration": { 
            "logDriver": "awslogs",
            "options": { 
               "awslogs-group" : "/ecs/vacation-java-task",
               "awslogs-region": "eu-central-1",
               "awslogs-stream-prefix": "ecs"
            }
         },
         "name": "java-task",
         "mountPoints": [
                {
                    "containerPath": "/efs",
                    "sourceVolume": "efs"
                }],
         "portMappings": [ 
            { 
               "containerPort": 8080,
               "hostPort": 8080,
               "protocol": "tcp"
            }
         ]
      }
   ],
   "volumes": [
        {
            "name": "efs",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-3e677566",
                "transitEncryption": "ENABLED"
            }
        }
    ],
   "cpu": "256",
   "executionRoleArn": "arn:aws:iam::478526318168:role/ecsTaskExecutionRole",
   "family": "java-task",
   "memory": "512",
   "networkMode": "awsvpc",
   "requiresCompatibilities": [ 
       "FARGATE"
    ]
}
